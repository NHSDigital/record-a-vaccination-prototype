{
  "name": "record-a-vaccination-prototype",
  "version": "1.0.0",
  "description": "A prototype of the NHS Record a vaccination service.",
  "main": "app.js",
  "scripts": {
    "build": "gulp build",
    "postinstall": "npm run build",
    "start": "node app.js",
    "prewatch": "gulp build",
    "watch": "gulp",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:fix": "npm run lint:js:fix && npm run lint:prettier:fix",
    "lint:prettier": "prettier --cache --cache-location .cache/prettier --cache-strategy content --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:js": "eslint --cache --cache-location .cache/eslint --cache-strategy content --color --ignore-path .gitignore \"**/*.js\"",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "author": "NHS England",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@inquirer/prompts": "^7.10.1",
    "@x-govuk/govuk-prototype-filters": "^2.0.3",
    "body-parser": "^2.2.0",
    "browser-sync": "^3.0.4",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "gulp": "^5.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-rename": "^2.1.0",
    "gulp-sass": "^6.0.1",
    "lodash": "^4.17.21",
    "nhsuk-frontend": "^10.1.0",
    "nunjucks": "^3.2.4",
    "path": "^0.12.7",
    "plugin-error": "^2.0.1",
    "portscanner": "^2.2.0",
    "sass-embedded": "1.93.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.12",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^6.6.0",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": "^22",
    "npm": "^11"
  }
}
